<h1>Listing places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Details</th>
      <th>Fees</th>
      <th>Fees</th>
      <th>Area</th>
      <th>Kind</th>
      <th>Activities</th>
      <th>Keyword</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
      <tr>
        <td><%= place.name %></td>
        <td><%= place.description %></td>
        <td><%= place.details %></td>
        <td><%= place.fees %></td>
        <td><%= place.fees %></td>
        <td><%= place.area %></td>
        <td><%= place.kind %></td>
        <td><%= place.activities %></td>
        <td><%= place.keyword %></td>
        <td><%= link_to 'Show', place %></td>
        <td><%= link_to 'Edit', edit_place_path(place) %></td>
        <td><%= link_to 'Destroy', place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
